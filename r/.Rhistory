library(dplyr)
library(tidyr)
library(purrr) # Sangat berguna untuk bekerja dengan list/file
# 1. Tentukan path ke folder Anda (atau biarkan "." untuk direktori kerja saat ini)
# dan temukan semua file .csv di dalamnya
path_folder <- "."
files_list <- list.files(path = path_folder,
pattern = "\\.csv$",  # Cari file yang berakhiran .csv
full.names = TRUE)   # Ambil nama lengkap filenya
# 2. Baca semua file CSV dan gabungkan menjadi satu data frame
# set_names() akan menggunakan nama file sebagai nama list
# map_dfr() akan membaca setiap file dan menggabungkannya,
# .id = "Nama_File" akan membuat kolom baru 'Nama_File'
all_data <- files_list %>%
set_names() %>%
map_dfr(~ read.csv(.), .id = "Nama_File")
# Opsional: Bersihkan nama file (hapus path folder jika ada)
all_data <- all_data %>%
mutate(Nama_File = basename(Nama_File))
# 3. Ubah ke format "panjang" (sama seperti sebelumnya)
data_long <- all_data %>%
pivot_longer(
cols = c(Tailwind.CSS, Bootstrap),
names_to = "Metode",
values_to = "Waktu"
)
# 4. Hitung statistik, tapi kali ini group by 'Nama_File' DAN 'Metode'
summary_stats <- data_long %>%
group_by(Nama_File, Metode) %>%
summarise(
`Rata-Rata` = mean(Waktu, na.rm = TRUE),
Median = median(Waktu, na.rm = TRUE),
`Std. Deviasi` = sd(Waktu, na.rm = TRUE),
Minimum = min(Waktu, na.rm = TRUE),
Maksimum = max(Waktu, na.rm = TRUE),
.groups = 'drop' # Lepas grouping setelah selesai
)
# 5. Lakukan "Pivot" ganda untuk mendapatkan format yang Anda inginkan
# Langkah A: Putar kolom statistik (Rata-Rata, Median, dll) menjadi baris
stats_long <- summary_stats %>%
pivot_longer(
cols = c(`Rata-Rata`, Median, `Std. Deviasi`, Minimum, Maksimum),
names_to = "Statistik",
values_to = "Nilai"
)
# Langkah B: Putar kolom 'Metode' (Bootstrap, Tailwind) menjadi kolom
final_table <- stats_long %>%
pivot_wider(
names_from = Metode,
values_from = Nilai
)
# 6. Tampilkan tabel akhir
print("Tabel Statistika Deskriptif Gabungan (per File):")
print(final_table)
